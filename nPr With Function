#include<stdio.h>
int factorial(int a);

int main(){
    int n , r ;

    printf("Enter The 1st Number\n");
    scanf("%d",&n);
    printf("Enter The 2nd Number\n");
    scanf("%d",&r);

    int npr = factorial(n)/factorial(n-r);

    printf("The nPr Of %d & %d is %d ",n , r , npr);

return 0;}

int factorial(int a){
    int fact = 1;
    for(int i = 1 ; i <= a ; i++){
        fact *= i;
    }
return fact;}
